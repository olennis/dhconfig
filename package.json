{
	"name": "@olennis/configs",
	"private": true,
	"scripts": {
		"build": "pnpm -r build",
		"lint": "pnpm -r lint",
		"test": "pnpm -r test",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml}\"",
		"format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yaml}\"",
		"lint:js": "eslint . --ext .js,.jsx,.ts,.tsx"
	},
	"devDependencies": {
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^3.2.0"
	}
}
